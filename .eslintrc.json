{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-unused-vars":"warn",
        "semi":"error",
        "no-console":"warn",
        "no-unused-expressions":"error",
        "no-empty":"error",
        "no-trailing-spaces":"error",
        "comma-dangle":"error",
        "no-undef":"error"
    }
}
